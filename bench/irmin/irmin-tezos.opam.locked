  "cppo" {= "1.6.9"}
  "csexp" {= "1.5.2"}
  "cstruct" {= "6.2.0"}
  "digestif" {= "1.1.4"}
  "dune" {= "3.8.2"}
  "dune-configurator" {= "3.8.2"}
  "either" {= "1.0.0"}
  "eqaf" {= "0.9"}
