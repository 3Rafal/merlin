  "lwt" {= "5.6.1"}
  "metrics" {= "0.4.1" & with-test}
  "metrics-unix" {= "0.4.1" & with-test}
  "mtime" {= "2.0.0"}
  "ocaml" {= "5.0.0"}
  "ocaml-base-compiler" {= "5.0.0"}
  "ocaml-compiler-libs" {= "v0.12.4"}
  "ocaml-config" {= "3"}
  "ocaml-options-vanilla" {= "1"}
  "ocaml-syntax-shims" {= "1.0.0"}
  "ocamlbuild" {= "0.14.2"}
  "ocamlfind" {= "1.9.6"}
  "ocamlgraph" {= "2.0.0"}
  "ocplib-endian" {= "1.2"}
  "optint" {= "0.3.0"}
  "ppx_derivers" {= "1.2.1"}
  "ppx_deriving" {= "5.2.1"}
  "ppx_irmin" {= "3.7.2"}
  "ppx_repr" {= "0.6.0"}
  "ppxlib" {= "0.27.0"}
  "progress" {= "0.2.2"}
  "psq" {= "0.2.1"}
  "re" {= "1.10.4" & with-test}
  "repr" {= "0.6.0"}
  "result" {= "1.5"}
